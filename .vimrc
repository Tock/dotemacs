set nocompatible
syntax enable
filetype on
set autoread
set autoindent smartindent
set expandtab
set smarttab
set tabstop=4
set shiftwidth=4
set backspace=eol,start,indent
set undolevels=1000
set complete=.,w,b,u,U,t,i,d
set hlsearch
set incsearch
set showmatch
colorscheme desert

"set insertmode
"set selectmode += cmd
